<div class="todo-list">
    <h1 class="list-title">Todo清单</h1>
    <ul class="list"></ul>
</div>
<div class="todo-act">
    <fieldset>
        <legend>Todo清单输入</legend>
        <input type="text" name="todo" placeholder="请输入你的todo！">
        <button type="button" class="remove">清除所有</button>
        <button type="button" class="check">标记所有</button>
        <button type="button" class="uncheck">清除所有标记</button>
    </fieldset>
</div>
